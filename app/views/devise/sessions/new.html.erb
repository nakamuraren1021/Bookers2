<h1>Log in</h1>
<%= form_for(resource, as: resource_name, url: session_path(resource_name)) do |f| %>
	 <div class="form-group2">
	   <label class="control-label" for="inputName">Name</label>
	   <br>
		<%= f.text_field :name, autofocus: true, class:"", id:"inputName", placeholder:""%>
	 </div>

	<div class="form-group2">
	 	<label class="control-label" for="inoputPassword">Password</label>
	 	<br>
		<%= f.password_field :password, autocomplete: "off", class:"", id:"inputPassword", placeholder:"" %>
	</div>
		<%= f.submit "Log in", class: "btn btn-primary" %>
<% end %>