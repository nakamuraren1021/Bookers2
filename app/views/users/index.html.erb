<%= render 'books/list', user: current_user  %>
<div class="col-xs-9">
<h2>Users</h2>
<table class="user-table">
   <thead>
	<tr>
		<th>image</th>
		<th>name</th>
		<th colspan="3"></th>
	</tr>
   </thead>
	<tbody>
		<% @user_all.each do |userall| %>
		<tr>
		<td><%= attachment_image_tag userall, :profile_image, :fill, 60, 60, class: "profile-thumb", fallback: "no_image.jpg" %></td>
		<td><%= userall.name %></td>
		<td><%= link_to "Show",user_path(userall) %></td>
	   </tr>
	<% end %>
	</tbody>
</table>
</div>
